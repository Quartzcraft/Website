  <templates>
    <template title="game.css" version_id="1" version_string="1.0.0"><![CDATA[a.block.gameAction {
    margin: 0 0 20px 0;
    padding: 15px 20px 15px 20px;
    width: 280px;
    font-size: 19px;
    background: #f1bb1a;
    line-height: 20px;
    color: #FFF;
    float: left;
    text-align: left;
    text-shadow: 0 1px 0 #e78807;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
}

a.block.gameAction:hover {
    opacity: 0.8;
}

a.block.gameAction i {
    float: right
}

div.serverInfo {
    width: 320px;
    height: 48px;
    margin: -15px 0 20px 0;
    background: #0099d7;
    color: #c9efff;
    float: left;
    -moz-border-bottom-left-radius: 4px;
    -moz-border-bottom-right-radius: 4px;
    -webkit-border-bottom-left-radius: 4px;
    -webkit-border-bottom-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}

div.serverInfo p {
    margin: 10px 0 0 20px;
    font-size: 19px;
    float: left;
}

div.serverInfo p i {
    float: left;
    margin: 4px 10px 0 0;
}

img.gameBanner {
	padding-bottom: 10px;
	max-width: 100%; 
	display:block; 
	height: auto;
}]]></template>
    <template title="games.css" version_id="1" version_string="1.0.0"><![CDATA[div.left div.games ul {
    float: left;
    margin: 0 0 20px 0
}

div.left div.games ul li {
    display: inline;
    float: left;
    width: 320px;
    height: 72px;
    background: #faf9f8;
    margin: 6px 0 0 0;
}

div.left div.games ul li img {
    width: 72px;
    height: 72px;
    float: left;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
}

div.left div.games ul li a {
    margin: 17px 0 0 18px;
    font-size: 20px;
    width: 200px;
    float: left;
}

div.left div.games ul li p {
    font-size: 14px;
    margin: 2px 0 0 18px;
    letter-spacing: -1px;
    color: #6d6d6d;
    width: 200px;
    float: left;
}]]></template>
    <template title="games_tab_links" version_id="2" version_string="20141222a"><![CDATA[<ul class="secondaryContent blockLinksList">
	<xen:if is="{$xenOptions.displayKingdoms}"><li><a href="{xen:link full:kingdoms}">{xen:phrase kingdoms}</a></li></xen:if>
	<xen:if is="{$xenOptions.displayHeist}"><li><a href="{xen:link full:the-heist}">{xen:phrase the_heist}</a></li></xen:if>
	<xen:if is="{$xenOptions.displayLabDefence}"><li><a href="{xen:link full:lab-defence}">{xen:phrase lab_defence}</a></li></xen:if>
</ul>]]></template>
    <template title="game_index" version_id="2" version_string="20141222a"><![CDATA[<xen:require css="game.css" />
<xen:require css="games.css" />
<xen:title>{xen:phrase games}</xen:title>
<xen:description>{xen:phrase games_description}</xen:description>

<div class="left">
	<div class="games">
		<ul>
			<xen:if is="{$xenOptions.displayKingdoms}">
				<li>
					<img src="https://minotar.net/avatar/nfell2009/72" alt="nfell2009" />
					<a href="{xen:link full:kingdoms}">{xen:phrase kingdoms}</a>
					<p>{xen:phrase kingdoms_description}</p>
				</li>
			</xen:if>
			<xen:if is="{$xenOptions.displayHeist}">
				<li>
