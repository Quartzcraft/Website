	</xen:if>
</div>
<div class="right narrow">
	<xen:if is="{$xenOptions.displayHeistGuide} && {$xenOptions.enableHeistGuidePage}">
		<a href="{xen:link 'the-heist/guide'}" class="block gameAction"><span>{xen:phrase guide}<i class="fa fa-book"></i></span></a>
	<xen:elseif is="{$xenOptions.displayHeistGuide} && !{$xenOptions.enableHeistGuidePage}" />
		<a href="{xen:link full:the-heist}#guide" class="block gameAction"><span>{xen:phrase guide}<i class="fa fa-book"></i></span></a>
	</xen:if>
	<xen:if is="{$xenOptions.displayHeistLeaderboard}">
		<a href="{xen:link 'the-heist/leaderboards'}" class="block gameAction"><span>{xen:phrase leaderboards}<i class="fa fa-trophy"></i></span></a>
	</xen:if>
	<xen:if is="{$xenOptions.displayHeistForumLink}">
		<a href="{$xenOptions.heistForumLink}" class="block gameAction"><span>{xen:phrase view} {xen:phrase forums}<i class="fa fa-folder"></i></span></a>
	</xen:if>
	<h3 class="sectionHead">{xen:phrase play_now}</h3>
	<div class="serverInfo">
		<p><i class="fa fa-globe"></i>play.QuartzCraft.co.uk</p>
	</div>
</div>]]></template>
